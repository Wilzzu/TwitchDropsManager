{
    "name": "Twitch Drops Manager",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "brrrr",
    "content_scripts": [{
        "matches": ["https://www.twitch.tv/*"],
        "js": ["drop.js"]
    }],
	
    "background": {
        "scripts": ["background.js"]
      },
    "options_page": "options.html",
    "permissions":[
        "storage",
        "tabs"
    ]
}